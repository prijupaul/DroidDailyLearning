<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.com.learning.droid.dailylearning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="au.com.learning.droid.dailylearning.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="au.com.learning.droid.dailylearning.Navigation.NavigationActivity">
        </activity>

        <activity android:name="au.com.learning.droid.dailylearning.Delegates.LoginActivity">

        </activity>

    </application>

</manifest>